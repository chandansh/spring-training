	public void setI(int i) {
		this.i = i;
	}

	RationalNumber() {
		this.type = RATIONAL;
	}
	
	@Override
	public void display() {
		System.out.println("");

	}

	@Override
	public Number add(Number number) {
		// TODO Auto-generated method stub
		return null;
